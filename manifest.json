{
    "name": "GIF Downloader",
    "description": "Download GIF's from using Giphy API",
    "manifest_version": 3,
    "version": "1.0",
    "action": {
        "default_popup": "popup.html"
    },
    "host_permissions": ["<all_urls>"],
    "permissions": [
        "downloads",  
        "contextMenus",
        "clipboardRead",
        "clipboardWrite",
        "scripting",
        "tabs",
        "activeTab"
    ]
}